<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
    <extension point="org.eclipse.edt.ide.core.generators">
		<provider
			id="org.eclipse.edt.gen.generator.example.ide.ExampleGeneratorProvider"
			class="org.eclipse.edt.gen.generator.example.ide.ExampleGenerator"
			name="%GeneratorName"
			compiler="org.eclipse.edt.ide.compiler.edtCompiler"
			version="1.0"
			provider="Eclipse"
			parentGeneratorId="org.eclipse.edt.ide.gen.JavaGenProvider"
			description="%GeneratorDescription"
		/>
   	</extension>
	<extension point="org.eclipse.edt.ide.ui.edtGeneratorTabs">
		<generatorTab 
			generatorId="org.eclipse.edt.gen.generator.example.ide.ExampleGeneratorProvider"
			class="org.eclipse.edt.gen.generator.example.ide.ExampleGeneratorTabProvider">
		</generatorTab>
   	</extension>
</plugin>
