		<provider
			id="org.eclipse.edt.gen.generator.example.ide.ExampleGeneratorProvider"
			class="org.eclipse.edt.gen.generator.example.ide.ExampleGeneratorProvider"
			name="%ExampleGeneratorName"
			compiler="org.eclipse.edt.ide.compiler.edtCompiler"
