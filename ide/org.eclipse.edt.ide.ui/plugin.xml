        	</action>
		</actionSet>
		<actionSet
            label="%EGLSearchActionSet.label"
            description="%EGLSearchActionSet.description"
            visible="false"
            id="org.eclipse.edt.ide.ui.SearchActionSet">
         	<action
               	label="%openEGLSearchPageAction.label"
               	icon="icons/ctool16/esrch_nav.gif"
               	class="org.eclipse.edt.ide.ui.internal.search.OpenEGLSearchPageAction"
               	menubarPath="org.eclipse.search.menu/dialogGroup"
               	id="org.eclipse.edt.ide.ui.search.OpenEGLSearchPage">
         	</action>      
         	<menu
               	label="%searchMenu.label"
               	id="org.eclipse.search.menu">
         	</menu>
      	</actionSet>         
	</extension>
	
	<extension
    	point="org.eclipse.ui.actionSetPartAssociations">
      	<actionSetPartAssociation
        	targetID="org.eclipse.edt.ide.ui.SearchActionSet">
         	<part
            	id="org.eclipse.edt.ide.ui.EGLEditor">
         	</part>
         	<part
            	id="org.eclipse.search.SearchResultView">
         	</part>
      	</actionSetPartAssociation>
   	</extension>
   	
	<extension 
		point="org.eclipse.ui.contexts">
		<context
        	name="%context.editingEGLSource.name"
            description="%context.editingEGLSource.description"
            parentId="org.eclipse.ui.textEditorScope"
            id="org.eclipse.edt.ide.ui.eglEditorScope">
      	</context>
	</extension>
	
	<extension 
		point="org.eclipse.ui.decorators">
    	<decorator adaptable="true"
            class="org.eclipse.edt.ide.ui.internal.packageexplorer.EGLProjectDecorator"
            id="org.eclipse.edt.ide.ui.EGLProjectDecorator"
            label="%EGLProjectDecorator.label"
            lightweight="true"
            state="true"
            location="TOP_RIGHT">
            <enablement>
	            <and>
		            <objectClass name="org.eclipse.core.resources.IProject"/>
		            <or>
		            	<objectState name="projectNature" value="org.eclipse.edt.ide.core.EGLNature"/>
		            </or>
	           </and>
         	</enablement>
      	</decorator>
   	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor
			name="%pgmEditorName"
			icon="icons/obj16/eglfile_obj.gif"
			extensions="egl"
			class="org.eclipse.edt.ide.ui.internal.editor.EGLEditor"
			symbolicFontName="org.eclipse.edt.ide.ui.editors.textfont"
			id="org.eclipse.edt.ide.ui.EGLEditor"
			default="true">
         </editor>
   		<editor
			id="org.eclipse.edt.ide.ui.BinaryFileEditor"
			default="true"
			name="%BinaryEditorName"
			icon="icons/obj16/eglfile_obj.gif"
			extensions="ir"
			class="org.eclipse.edt.ide.ui.internal.editor.BinaryFileEditor" />   
	</extension>
	
	<!--
***********************************************************************
Templates
	To add a new egl component, for templates you need to:
	- add a new contextType and include element here
	- create a new templates.xml file and templates.properties file
	- create a new org.eclipse.edt.ide.ui.internal.templates.xxxContextType.Java file
	- register the xxxContextType in Activator.getTemplateContextRegistry()
	- update org.eclipse.edt.ide.ui.internal.templates.EGLTemplateStore.hasCapabilityEnabled()
***********************************************************************
-->
	<extension point="org.eclipse.ui.editors.templates">
		<contextType
        	name="%CoreTemplates"
            class="org.eclipse.edt.ide.ui.internal.templates.CoreContextType"
            id="egl-core">
      	</contextType>
      	<include
      		file="templates/eglcoredefault-templates.xml"
      		translations="templates/eglcoredefault-templates.properties">
      	</include>
      	<contextType
            name="%RUITemplates"
            class="org.eclipse.edt.ide.ui.internal.templates.RUIContextType"
            id="egl-rui">
      	</contextType>
      	<include
      		file="templates/eglruidefault-templates.xml"
      		translations="templates/eglruidefault-templates.properties">
      	</include>
	</extension>

	<extension point="org.eclipse.ui.ide.projectNatureImages">
      	<image
            icon="icons/ovr16/egl_ovr.gif"
            natureId="org.eclipse.edt.ide.core.EGLNature"
            id="org.eclipse.edt.ide.ui.eglProjectNatureImage">
      	</image>
	</extension>	
	
	<extension point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
      		activeByDefault="true"
            contentProvider="org.eclipse.edt.ide.ui.internal.packageexplorer.NavigatorContentProvider"      		
            icon="icons/cview16/eglperspective.gif"            
            id="org.eclipse.edt.ide.ui.navigator.EGLContent"
            labelProvider="org.eclipse.edt.ide.ui.internal.packageexplorer.EGLElementLabelProvider"
            name="EGL"
            priority="higher">
			<triggerPoints>
				<or>						
					<adapt
							type="org.eclipse.core.resources.IProject" >
						<test
							property="org.eclipse.core.resources.projectNature"
							value="org.eclipse.edt.ide.core.EGLNature" />
					</adapt>
<!--					
					<instanceof
							value="org.eclipse.edt.ide.core.model.IEGLProject"/>
-->							
					<instanceof
							value="org.eclipse.edt.ide.core.model.IEGLElement" />
				</or>	
			</triggerPoints>
			<possibleChildren>
				<or>
					<instanceof
							value="org.eclipse.core.resources.IFolder" />
					<instanceof
							value="org.eclipse.core.resources.IFile" />				
					<instanceof
							value="org.eclipse.edt.ide.core.model.IEGLProject"/>							
					<instanceof
							value="org.eclipse.edt.ide.core.model.IEGLElement" />
				</or>
			</possibleChildren>
			<override
					policy="InvokeAlwaysRegardlessOfSuppressedExt"
					suppressedExtensionId="org.eclipse.ui.navigator.resourceContent"/>
			<commonSorter
					class="org.eclipse.edt.ide.ui.internal.packageexplorer.EGLElementSorter"
					id="org.eclipse.edt.ide.ui.packageexplorer.EGLElementSorter"/>   				
		    <actionProvider 
		    		id="org.eclipse.edt.ide.ui.packageexplorer.OpenActions"
					class="org.eclipse.edt.ide.ui.internal.packageexplorer.OpenActionProvider"
    		        overrides="org.eclipse.ui.navigator.resources.OpenActions">
					<enablement>
						<or>
							<instanceof value="org.eclipse.edt.ide.core.model.IEGLElement" /> 	
						</or>
					</enablement>					
			</actionProvider> 	
			<actionProvider
		            class="org.eclipse.edt.ide.ui.internal.packageexplorer.EditActionGroup"
		            overrides="org.eclipse.jdt.internal.ui.navigator.PackageExplorerActionProvider">
		<!--        overrides="org.eclipse.ui.navigator.resources.actions.RefactorActions"> -->
		         	<enablement>
		         		<or>
							<instanceof value="org.eclipse.edt.ide.core.model.IEGLElement" />	         	
						</or>
		         	</enablement>
		    </actionProvider>	
			<!-- Add wizards to New context menu of Project Explorer view -->
			<commonWizard
				type="new"
				menuGroupId="org.eclipse.edt.ide.ui.wizards.menuGroup"
				wizardId="org.eclipse.edt.ide.ui.newLibraryWizard">
				<enablement>
 		        	<or>
	 					<adapt
								type="org.eclipse.core.resources.IProject" >
							<test
								property="org.eclipse.core.resources.projectNature"
								value="org.eclipse.edt.ide.core.EGLNature" />
						</adapt>
						<instanceof
								value="org.eclipse.edt.ide.core.model.IEGLElement" />
 		        	</or>         					
				</enablement>
			</commonWizard>
			<commonWizard
				type="new"
				menuGroupId="org.eclipse.edt.ide.ui.wizards.menuGroup"
				wizardId="org.eclipse.edt.ide.ui.newPackageWizard">
				<enablement>
 		        	<or>
	 					<adapt
								type="org.eclipse.core.resources.IProject" >
							<test
								property="org.eclipse.core.resources.projectNature"
								value="org.eclipse.edt.ide.core.EGLNature" />
						</adapt>
						<instanceof
								value="org.eclipse.edt.ide.core.model.IEGLElement" />
 		        	</or>         					
				</enablement>
			</commonWizard>
			<commonWizard
            	type="new"
            	menuGroupId="org.eclipse.edt.ide.ui.project.wizards.menuGroup"
            	wizardId="org.eclipse.edt.ide.ui.newProjectWizard">
         		<enablement>
            		<or>
            			<adapt
								type="org.eclipse.core.resources.IProject" >
							<test
								property="org.eclipse.core.resources.projectNature"
								value="org.eclipse.edt.ide.core.EGLNature" />
						</adapt>
               			<and>
					  		<instanceof value="java.util.Collection"/>
					  		<count value="0" />
						</and>	
               			<instanceof value="org.eclipse.core.resources.IWorkspaceRoot" />
            		</or>
         		</enablement>
      		</commonWizard>		      
			<commonWizard
				type="new"
				menuGroupId="org.eclipse.edt.ide.ui.wizards.menuGroup"
				wizardId="org.eclipse.edt.ide.ui.newProgramWizard">
				<enablement>
 		        	<or>
	 					<adapt
								type="org.eclipse.core.resources.IProject" >
							<test
								property="org.eclipse.core.resources.projectNature"
								value="org.eclipse.edt.ide.core.EGLNature" />
						</adapt>
						<instanceof
								value="org.eclipse.edt.ide.core.model.IEGLElement" />
 		        	</or>         					
				</enablement>
			</commonWizard>			
			<commonWizard
				type="new"
				menuGroupId="org.eclipse.edt.ide.ui.wizards.menuGroup"
				wizardId="org.eclipse.edt.ide.ui.newRuiHandlerWizard">
				<enablement>
 		        	<or>
	 					<adapt
								type="org.eclipse.core.resources.IProject" >
							<test
								property="org.eclipse.core.resources.projectNature"
								value="org.eclipse.edt.ide.core.EGLNature" />
						</adapt>
						<instanceof
								value="org.eclipse.edt.ide.core.model.IEGLElement" />
 		        	</or>         					
				</enablement>
			</commonWizard>	
			<commonWizard
				type="new"
				menuGroupId="org.eclipse.edt.ide.ui.wizards.menuGroup"
				wizardId="org.eclipse.edt.ide.ui.newRuiWidgetWizard">
				<enablement>
 		        	<or>
	 					<adapt
								type="org.eclipse.core.resources.IProject" >
							<test
								property="org.eclipse.core.resources.projectNature"
								value="org.eclipse.edt.ide.core.EGLNature" />
						</adapt>
						<instanceof
								value="org.eclipse.edt.ide.core.model.IEGLElement" />
 		        	</or>         					
				</enablement>
			</commonWizard>
			<commonWizard
				type="new"
				menuGroupId="org.eclipse.edt.ide.ui.wizards.menuGroup"
				wizardId="org.eclipse.edt.ide.ui.newSourceFileWizard">
				<enablement>
 		        	<or>
	 					<adapt
								type="org.eclipse.core.resources.IProject" >
							<test
								property="org.eclipse.core.resources.projectNature"
								value="org.eclipse.edt.ide.core.EGLNature" />
						</adapt>
						<instanceof
								value="org.eclipse.edt.ide.core.model.IEGLElement" />
 		        	</or>         					
				</enablement>
			</commonWizard>		
	      </navigatorContent>
      <commonFilter
            class="org.eclipse.edt.ide.ui.internal.packageexplorer.EmptyEGLInnerPackageFilter"                  
            description="%EmptyEGLPackages.description"
            id="org.eclipse.edt.ide.ui.packageexplorer.filter.EmptyEGLInnerPackageFilter"            
