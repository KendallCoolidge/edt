<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension-point id="eglFoldingStructureProviders" name="EGL Folding Structure Provider" schema="schema/eglFoldingStructureProviders.exsd"/>
	<extension-point id="edtGeneratorTabs" name="EDT Generator Tabs" schema="schema/edtGeneratorTabs.exsd"/>
	<extension-point id="projectType" name="New EGL Project Type" schema="schema/projectType.exsd"/>
	   
	<extension 
		point="org.eclipse.core.runtime.preferences">
        <initializer class="org.eclipse.edt.ide.ui.internal.EDTUIPreferenceInitializer"/>
    </extension>
    
    <!-- EGL Editor Folding provider -->
	<extension
		point= "org.eclipse.edt.ide.ui.eglFoldingStructureProviders">
		<provider
			id="org.eclipse.edt.ide.ui.internal.editor.folding.eglFoldingStructureProviders"
			name= "%DefaultEGLFoldingStructureProviderName"
			class="org.eclipse.edt.ide.ui.internal.editor.folding.FoldingStructureProvider"
			preferencesClass="org.eclipse.edt.ide.ui.internal.editor.folding.FoldingPreferenceBlock">
		</provider>
	</extension>
    
	<extension point="org.eclipse.ui.bindings">
		<!-- Navigate -->
		<key
            commandId="org.eclipse.edt.ide.ui.navigate.openOnSelection"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.edt.ide.ui.eglEditorScope"
            sequence="F3"/>
        <!-- Source import -->
       	<key
            commandId="org.eclipse.edt.ide.ui.source.importGroup.addImport"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.edt.ide.ui.eglEditorScope"
            sequence="Ctrl+Shift+M"/>
       	<key
            commandId="org.eclipse.edt.ide.ui.source.importGroup.organizeImports"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.edt.ide.ui.eglEditorScope"
            sequence="Ctrl+Shift+O"/>
        <!-- Source edit -->
       	<key
            commandId="org.eclipse.edt.ide.ui.source.editGroup.format"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.edt.ide.ui.eglEditorScope"
            sequence="Ctrl+Shift+F"/>
        <!-- Source edit SQL -->
<!-- Uncomment when SQL support is ready
       	<key
            commandId="org.eclipse.edt.ide.ui.source.sqlGroup.addSQLStatement"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.edt.ide.ui.eglEditorScope"
            sequence="Ctrl+Shift+A"/>
-->            
  	</extension>
	
	<extension 
		point="org.eclipse.ui.commands">
		<!-- Navigate -->
		<command
	  		name="%OpenOnSelection.name"
			description="%OpenOnSelection.description"	  		
			id="org.eclipse.edt.ide.ui.navigate.openOnSelection"/>
 		<!-- Source import -->
		<command
	  		name="%AddImport.name"
			description="%AddImport.description"	  		
			id="org.eclipse.edt.ide.ui.source.importGroup.addImport"/>
		<command
	  		name="%OrganizeImports.name"
			description="%OrganizeImports.description"	  		
			id="org.eclipse.edt.ide.ui.source.importGroup.organizeImports"/>
		<!-- Source edit -->
		<command
	  		name="%Format.name"
			description="%Format.description"	  		
			id="org.eclipse.edt.ide.ui.source.editGroup.format"/>
		<!-- Source edit SQL -->
<!-- Uncomment when SQL support is ready
		<command
	  		name="%AddSQLStatement.name"
			description="%AddSQLStatement.description"	  		
			id="org.eclipse.edt.ide.ui.source.sqlGroup.addSQLStatement"/>
-->			
		<!-- Ruler -->
		<command
	  		name="%Folding.name"
			description="%Folding.description"	  		
			id="org.eclipse.edt.ide.ui.ruler.folding"/>
	</extension>   
	
	<extension 
		point="org.eclipse.ui.menus">
		<menuContribution locationURI="menu:navigate?after=open.ext">
			<command commandId="org.eclipse.edt.ide.ui.navigate.openOnSelection">
       	 		<visibleWhen>
       				<with variable="activePartId">
       					<equals value="org.eclipse.edt.ide.ui.EGLEditor"/>
       				</with>
     			</visibleWhen>
     		</command>
  		</menuContribution>
  		<menuContribution locationURI="menu:org.eclipse.ui.main.menu?after=edit">
     		<menu id="org.eclipse.edt.ide.ui.source.menu" 
     			label="%SourceMenu.name">
       	 		<command commandId="org.eclipse.edt.ide.ui.source.editGroup.format"/>
       	 		<separator name="importGroup" visible="true"/>
       	 		<command commandId="org.eclipse.edt.ide.ui.source.importGroup.addImport"/>
   	 			<command commandId="org.eclipse.edt.ide.ui.source.importGroup.organizeImports"/>
<!-- Uncomment when SQL support is ready
   	 			<separator name="sqlGroup" visible="true"/>
   	 			<command commandId="org.eclipse.edt.ide.ui.source.sqlGroup.addSQLStatement"/>
-->   	 			
       	 		<separator name="additions" visible="false"/>
       	 		<visibleWhen>
       				<with variable="activePartId">
           				<equals value="org.eclipse.edt.ide.ui.EGLEditor"/>
           			</with>
     			</visibleWhen>
    	 	</menu>
  		</menuContribution>
  		<menuContribution locationURI="popup:#EGLEditorContext">
			<command commandId="org.eclipse.edt.ide.ui.navigate.openOnSelection">
				<visibleWhen>
       				<with variable="activePartId">
       					<equals value="org.eclipse.edt.ide.ui.EGLEditor"/>
       				</with>
     			</visibleWhen>
     		</command>
 		</menuContribution>
  		<menuContribution locationURI="popup:#EGLEditorContext">
     		<menu id="org.eclipse.edt.ide.ui.popup.source.menu" 
     			label="%SourceMenu.name">
       	 		<command commandId="org.eclipse.edt.ide.ui.source.editGroup.format"/>
       	 		<separator name="importGroup" visible="true"/>
       	 		<command commandId="org.eclipse.edt.ide.ui.source.importGroup.addImport"/>
       	 		<command commandId="org.eclipse.edt.ide.ui.source.importGroup.organizeImports"/>
<!-- Uncomment when SQL support is ready
       	 		<separator name="sqlGroup" visible="true"/>
   	 			<command commandId="org.eclipse.edt.ide.ui.source.sqlGroup.addSQLStatement"/>
-->
       	 		<separator name="additions" visible="false"/>
       	 		<visibleWhen>
       				<with variable="activePartId">
           				<equals value="org.eclipse.edt.ide.ui.EGLEditor"/>
           			</with>
     			</visibleWhen>
    	 	</menu>
  		</menuContribution>
  			<menuContribution locationURI="popup:#EGLRulerContext">
			<command commandId="org.eclipse.edt.ide.ui.ruler.folding">
				<visibleWhen>
       				<with variable="activePartId">
       					<equals value="org.eclipse.edt.ide.ui.EGLEditor"/>
       				</with>
     			</visibleWhen>
     		</command>
 		</menuContribution>
	</extension>
	
	<extension 
		point="org.eclipse.ui.handlers">
		<handler
        	class="org.eclipse.edt.ide.ui.internal.handlers.OpenOnSelectionHandler"
        	commandId="org.eclipse.edt.ide.ui.navigate.openOnSelection">
   		</handler>
  		<handler
        	class="org.eclipse.edt.ide.ui.internal.handlers.FormattingHandler"
        	commandId="org.eclipse.edt.ide.ui.source.editGroup.format">
   		</handler>
   		<handler
        	class="org.eclipse.edt.ide.ui.internal.handlers.AddImportHandler"
        	commandId="org.eclipse.edt.ide.ui.source.importGroup.addImport">
  		</handler>
  		<handler
        	class="org.eclipse.edt.ide.ui.internal.handlers.OrganizeImportsHandler"
        	commandId="org.eclipse.edt.ide.ui.source.importGroup.organizeImports">
  		</handler>
<!-- Uncomment when SQL support is ready 		
  		<handler
        	class="org.eclipse.edt.ide.ui.internal.handlers.AddSQLStatementHandler"
        	commandId="org.eclipse.edt.ide.ui.source.sqlGroup.addSQLStatement">
  		</handler>
-->  		
  		<handler
        	class="org.eclipse.edt.ide.ui.internal.handlers.FoldingHandler"
        	commandId="org.eclipse.edt.ide.ui.ruler.folding">
  		</handler>
	</extension>
	
	<extension
    	point="org.eclipse.compare.contentViewers">
		<viewer
        	extensions="egl"
            class="org.eclipse.edt.ide.ui.internal.compare.TextViewerCreator"
            id="org.eclipse.edt.ide.ui.internal.ui.compare.TextViewerCreator">
      	</viewer>
      	<contentTypeBinding
			contentTypeId="org.eclipse.edt.ide.ui.egl"
            contentViewerId="org.eclipse.edt.ide.ui.internal.compare.TextViewerCreator">
	  	</contentTypeBinding>
	</extension>
   
	<extension
        point="org.eclipse.compare.contentMergeViewers">
      	<viewer
            extensions="egl"
            class="org.eclipse.edt.ide.ui.internal.compare.ContentViewerCreator"
            id="org.eclipse.edt.ide.ui.internal.compare.ContentViewerCreator">
      	</viewer>
      	<contentTypeBinding
			contentTypeId="org.eclipse.edt.ide.ui.egl"
            contentMergeViewerId="org.eclipse.edt.ide.ui.internal.compare.ContentViewerCreator">
	  	</contentTypeBinding>
   	</extension>
   	
   	<extension
    	point="org.eclipse.ui.actionSets">
        <actionSet
            label="%EGLActionSet.label"
            description="%EGLActionSet.description"
            visible="false"
            id="org.eclipse.edt.ide.ui.EGLActionSet">
       		<action
               	definitionId="org.eclipse.edt.ide.ui.navigate.openPart"
               	label="%OpenPartAction.label"
               	icon="icons/ctool16/opnprt_nav.gif"
               	class="org.eclipse.edt.ide.ui.internal.actions.OpenPartAction"
              	tooltip="%OpenPartAction.tooltip"
               	menubarPath="navigate/open.ext2"
               	toolbarPath="org.eclipse.search.searchActionSet/Search"
               	id="org.eclipse.edt.ide.ui.search.OpenPart">
        	</action>
        </actionSet>
		<actionSet
      		label="%EGLProjectWizardActionSet.label"
            description="%EGLProjectWizardActionSet.description"
            visible="false"
            id="org.eclipse.edt.ide.ui.EGLProjectWizardActionSet">
 			<action
         	   	definitionId="org.eclipse.edt.ide.ui.newProgramWizard"
           		label="%ProgramAction.label"
            	icon="icons/ctool16/newpgm_wiz.gif"
