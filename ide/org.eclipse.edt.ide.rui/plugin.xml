<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension-point id="widgetLibraryProvider" name="Widget Library Provider" schema="schema/widgetLibraryProvider.exsd"/>
<!--
***********************************************************************
Register the Preference Pages
***********************************************************************
-->
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
      		name="%RUI_Preference_Page_Name"
      		category="org.eclipse.edt.ide.ui.preferences"
      		class="org.eclipse.edt.ide.rui.internal.preferences.RUIPreferencePage"
      		id="org.eclipse.edt.ide.rui.ruiPreferences">
      </page>
      <page
            category="org.eclipse.edt.ide.ui.preferences"
            class="org.eclipse.edt.ide.rui.internal.testserver.TestServerPreferencePage"
            id="org.eclipse.edt.ide.rui.testServerPreferences"
            name="%TestServerPreferencePageName">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.eclipse.edt.ide.rui.internal.RUIPReferenceInitializer"/>
   </extension>
   <extension
         point="org.eclipse.edt.ide.ui.projectTemplates">
      <template
            category="org.eclipse.edt.ide.ui.projects"
            default="false"
            description="%EGLWebClientProjectDescription"
            icon="icons/browser.gif"
            id="org.eclipse.edt.ide.ui.projects.webClient"
            name="%EGLWebClientProject"
            templateClass="org.eclipse.edt.ide.rui.internal.project.WebClientProjectTemplate"
            wizardClass="org.eclipse.edt.ide.rui.wizards.WebClientProjectTemplateWizard">
      </template>
      <template
            category="org.eclipse.edt.ide.ui.projects"
            default="false"
            description="%EGLWebClientWithServicesProjectDescription"
            icon="icons/web.gif"
            id="org.eclipse.edt.ide.ui.projects.webClientWithServices"
            name="%EGLWebClientWithServicesProject"
            templateClass="org.eclipse.edt.ide.rui.internal.project.WebClientWithServicesProjectTemplate"
            wizardClass="org.eclipse.edt.ide.rui.wizards.WebClientWithServicesProjectTemplateWizard">
      </template>
      
      <template
            category="org.eclipse.edt.ide.ui.projects"
            default="false"
            description="%RUIHelloWorldProjectDescription"
            icon="icons/helloworld.gif"
            id="org.eclipse.edt.ide.ui.projects.rui.helloworld"
            name="%RUIHelloWorldProject"
            templateClass="org.eclipse.edt.ide.rui.internal.project.HelloWorldProjectTemplate" >
      </template>
            
   </extension>
<!--                                                                                 -->
<!-- Define the widget library provider extension point                              -->
<!--                                                                                 -->
<!-- =============================================================================== -->
   <extension
         point="org.eclipse.edt.ide.rui.widgetLibraryProvider">
      <provider
            detail="%StandardWidgetLibraryDescription"
            id="org.eclipse.edt.rui.widgets_0.7.0"
            importClass="org.eclipse.edt.ide.rui.internal.project.DefaultWidgetLibraryImporter"
            libname="%StandardWidgetLibraryName"
            logo=""
            projectName="org.eclipse.edt.rui.widgets_0.7.0"
            provider="Eclipse.org"
            resourceFolder="/org.eclipse.edt.ide.ui.rui/"
            resourcePluginName="org.eclipse.edt.ide.ui.resources"
            selected="true"
            version="0.7.0">
      </provider>
      <provider
            conflictClass="org.eclipse.edt.ide.rui.internal.project.DojoConflictClass"
            detail="%DojoWidgetLibraryDescription"
            id="org.eclipse.edt.rui.dojo_0.7.0"
            importClass="org.eclipse.edt.ide.rui.internal.project.DefaultWidgetLibraryImporter"
            libname="%DojoWidgetLibraryName"
            logo="icons/dojo16_logo.png"
            projectName="org.eclipse.edt.rui.dojo_0.7.0"
            provider="Eclipse.org"
             resourceFolder="/org.eclipse.edt.ide.ui.rui/"
            resourcePluginName="org.eclipse.edt.ide.ui.resources"
            selected="true"
            version="0.7.0"
            version_desc="Dojo 1.6.1">
      </provider>
      <provider
            conflictClass="org.eclipse.edt.ide.rui.internal.project.DojoConflictClass"
            detail="%DojoWidgetLibraryDescription"
            id="org.eclipse.edt.rui.dojo.remote_0.7.0"
            importClass="org.eclipse.edt.ide.rui.internal.project.DefaultWidgetLibraryImporter"
            libname="%DojoWidgetLibraryName"
            logo="icons/dojo16_logo.png"
            projectName="org.eclipse.edt.rui.dojo.remote_0.7.0"
            provider="Eclipse.org"
            resourceFolder="/org.eclipse.edt.ide.ui.rui/"
            resourcePluginName="org.eclipse.edt.ide.ui.resources"
            selected="false"
            version="0.7.0"
            version_desc="Dojo 1.6.1 Remote">
      </provider>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            id="org.eclipse.edt.ide.rui.testServerLaunchType"
            name="%TestServerConfigTypeName"
            public="false">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="org.eclipse.edt.ide.rui.testServerLaunchType"
            icon="icons/full/obj16/egl_launch_obj.gif"
            id="org.eclipse.edt.ide.rui.testServerLaunchImage">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchDelegates">
      <launchDelegate
            delegate="org.eclipse.edt.ide.rui.internal.testserver.TestServerLaunchDelegate"
            id="org.eclipse.edt.ide.rui.testServerLaunchDelegate"
            modes="run,debug"
            name="%TestServerConfigTypeName"
            sourcePathComputerId="org.eclipse.jdt.launching.sourceLookup.javaSourcePathComputer"
            type="org.eclipse.edt.ide.rui.testServerLaunchType">
         <modeCombination
               modes="run, debug"
               perspective="org.eclipse.debug.ui.DebugPerspective">
         </modeCombination>
         <modeCombination
               modes="debug"
               perspective="org.eclipse.debug.ui.DebugPerspective">
         </modeCombination>
      </launchDelegate>
   </extension>   
</plugin>
