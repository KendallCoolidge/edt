package gallery.welcome;

import org.eclipse.edt.rui.widgets.Box;
import org.eclipse.edt.rui.widgets.HTML;
import eglx.ui.rui.RUIHandler;
import gallery.lib.samplesMessages;


handler Welcome type RUIhandler {initialUI = [ui], cssFile="css/org.eclipse.edt.rui.samples.css"}
	ui Box {
		// PROPERTIES
		// The welcome text, like all the overview text (see samplesLib.egl) comes from the
		// properties library.  See the samplesMessages library for more details.
		children = [new HTML{text = samplesMessages.WelcomeText}]
	};
end
