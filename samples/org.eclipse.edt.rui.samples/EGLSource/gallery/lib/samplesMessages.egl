package gallery.lib;

import eglx.ui.rui.RUIPropertiesLibrary;

// PROPERTIES
// Here's our properties library.  When our RUI handler starts up, these variables
// are automatically populated with the contents defined in samplesMessages.properties,
// in the WebContent/properties folder.
library samplesMessages type RUIPropertiesLibrary
	WelcomeOverview string;
	WelcomeText string;
	HelloWorldOverview string;
	EventsOverview string;
	ValidationOverview string;
	DataDisplayOverview string;
	DragDropOverview string;
	ServicesOverview string;
	ServicesText string;
	RSSOverview string;
	AdvancedOverview string;
	AdvancedText string;
end
