package gallery.rss;

import eglx.http.HttpMethod;
import eglx.rest.Rest;



// interface
Library NewsFeedProxyLib

	function getRSS20(uri string in) returns (RSS20){ 
		@Rest { 
			method = HttpMethod._GET,
			uriTemplate="{uri}"
		}
	}
	end
end
