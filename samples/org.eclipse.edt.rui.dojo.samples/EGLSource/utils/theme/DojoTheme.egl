package utils.theme;

import eglx.javascript.JavaScriptObject;
import eglx.ui.rui.VEProperty;

ExternalType DojoTheme type JavaScriptObject {
		relativePath = "utils/theme",
		externalName = "DojoTheme"
	}
	
	/*
	 * The currently selected Dojo theme to use for styling the Dojo widgets
	 */
	theme String{ @Property, @VEProperty{}}; 

	/*
	 * The location where dojo is loaded from. Internal use only.
	 */
	baseURL String;
end
