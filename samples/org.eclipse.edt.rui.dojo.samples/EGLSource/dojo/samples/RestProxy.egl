package dojo.samples;

import eglx.http.HttpMethod;
import eglx.rest.Rest;

// basic handler

library RestProxy

    function invokeGet(reqURL string in) returns(string){
    	@Rest{
    		method = HttpMethod._GET,
    		uriTemplate="{reqURL}"
    	}
    }
   	end


end
