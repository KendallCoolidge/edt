package utils.map;

library GoogleMapLib
	
	const HYBRID string = "hybrid";
	const ROADMAP string = "roadmap";
	const SATELLITE string = "satellite";
	const TERRAIN string = "terrain";
	
	function toDecimalDegrees(deg int, min int, sec int) returns (float)
		return (deg + (min/60) + (sec/3600));
	end
	
end
