	{relativePath = "org/eclipse/edt/rui/mvc",
	 externalName = "InternalFormattingUtil"}
	 
	function decimalValueIsValid(a any in, length int in, decimals int in) returns(boolean);
	function getCurrencySymbol() returns(string);
	function getDecimalSeparator() returns(string);
	function getGroupingSeparator() returns(string);
	function asDate(s String in, mask String in, strict boolean in) returns(date);
	function asTime(s String in, mask String in, strict boolean in) returns(time);
	function asTimestamp(s String in, mask String in, strict boolean in)returns(timestamp);
