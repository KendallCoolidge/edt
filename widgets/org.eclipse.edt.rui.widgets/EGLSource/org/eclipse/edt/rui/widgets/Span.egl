package org.eclipse.edt.rui.widgets;

Handler Span type RUIWidget{tagName = "span", cssFile = "css/org.eclipse.edt.rui.css" }

	text String  { @EGLProperty { getMethod=getText, setMethod=setText } };

	function setText(textIn String in)
		innerHTML = textIn;
	end
	
	function getText() returns (String)
		return (innerHTML);
	end

end
