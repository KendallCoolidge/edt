package dojo.widgets;

import eglx.javascript.javascriptobject;
import eglx.lang.property;
import eglx.ui.rui.eventhandler;
import eglx.ui.rui.ruilib;
import eglx.ui.rui.veevent;
import eglx.ui.rui.veproperty;
import eglx.ui.rui.vewidget;

/*
 * This type is used by all Dojo chart widget, and abstracts out all the common
 * functionality found in charts. 
 * 
 * This is an internal type. Do not instantiate directly.
 * 
 * For more information on Dojo charts, see http://docs.dojocampus.org/dojox/charting
 *
 */
ExternalType DojoChartBase extends DojoBase type JavaScriptObject {
	relativePath = "dojo/widgets",
	externalName = "DojoChartBase"
}

	/*
	 * redraw the chart to fix rendering issues that may appear on IE
	 */
	function redraw();
	
end
