<?xml version="1.0" encoding="UTF-8"?>
<egl:deployment xmlns:egl="http://www.eclipse.org/xmlns/edt/deployment/1.0">
  <bindings>
    <binding name="DerbyConnection" type="edt.binding.sql" uri="workspace://DerbyConnection" useURI="true"/>
    <binding name="LocalDB2" type="edt.binding.sql" uri="workspace://LocalDB2" useURI="true">
      <parameters>
        <parameter name="deployAsJndi" value="false"/>
        <parameter name="jndiName" value="jdbc/LocalDB2"/>
      </parameters>
    </binding>
  </bindings>
</egl:deployment>