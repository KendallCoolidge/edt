/** @test
  * @name EBoolean006
  * @title EBoolean006 - runISAFunction
  * @description testing boolean nullable variable with the isa operator
  * @testcases runISAFunction01 - runISAFunctionn29
  * @sources EBoolean006.egl
  * @keywords boolean, isa
 **/
library EBoolean006 {}

	isIt boolean;
	expected boolean;
	boolean1n boolean? = true;
	
	variation string;
	
	function assertTrue()
	 	success boolean = (isIt == expected);
	 	if (!success)
	 		Syslib.writeStdOut(variation);
	 		SysLib.writeStdOut(" actual " + isIt);
	 		SysLib.writeStdOut(" expect " + expected);
	 	end
 		LogResult.assertTrue(variation, success);
 	end

	function runISAFunction01(){@Test}
		variation = "boolean/nullable  variable is any";
		isIt = boolean1n isa any;
		expected = false;
		assertTrue();
	end
	
	function runISAFunction02(){@Test}
		variation = "boolean/nullable  variable is bigInt";
		isIt = boolean1n isa bigInt;
		expected = false;
		assertTrue();
	end
	
	function runISAFunction03(){@Test}
		variation = "boolean/nullable  variable is boolean";
		isIt = boolean1n isa boolean;
		expected = true;
		assertTrue();
	end
	
	function runISAFunction04(){@Test}
