package expressions;

// basic library
library bitwise
	
	function negate()
		i int;
		si smallint;
		a any?;
		s string;
		
		i = ~si; //0
		si = ~i; //0
		i = ~2343; //0
		si = ~2; //0
		i = ~funcIn(~i); //0
		i = ~funcInout(i); //0
		
		a = ~"23"; //1
		a = ~a; //1
		a = ~s; //1
		
		funcIn(~i); //0
		funcIn(~si); //0
		funcIn(~222); //0
		
		funcInout(~i); //1
		funcInout(~si); //1
		funcInout(~222); //1
	end
	
	function shiftLeft()
		i int;
		si smallint;
		s string;
		a any?;
		
		i = si << 2; // 0
		i = i << si; // 0
		si = i << 2; // 0
		si = si << i; // 0
		si = getInt() << getSmallint(); //0
		a = s << 2; //1
		a = 2 << a; //1
		
		i <<= 2; // 0
		i <<= si; // 0
		i <<= i; // 0
		i <<= a; // 1
		i <<= s; // 1
		i <<= "2"; // 1
		i <<= getString(); //1
		
		si <<= 2; // 0
		si <<= i; // 0
		si <<= si; // 0
		si <<= getInt(); // 0
		si <<= a; // 1
		si <<= s; // 1
		si <<= "2"; // 1
		
		s <<= 2; //1 
		a <<= 2; //1
	end
	
	function shiftRightArithmetic()
		i int;
		si smallint;
		s string;
		a any?;
		
		i = si >> 2; // 0
		i = i >> si; // 0
		si = i >> 2; // 0
		si = si >> i; // 0
		si = getInt() >> getSmallint(); //0
		a = s >> 2; //1
		a = 2 >> a; //1
		
		i >>= 2; // 0
		i >>= si; // 0
		i >>= i; // 0
		i >>= a; // 1
		i >>= s; // 1
		i >>= "2"; // 1
		i >>= getString(); //1
		
		si >>= 2; // 0
		si >>= i; // 0
		si >>= si; // 0
		si >>= getInt(); // 0
		si >>= a; // 1
		si >>= s; // 1
		si >>= "2"; // 1
		
		s >>= 2; //1 
		a >>= 2; //1
	end
	
	function shiftRightLogical()
		i int;
		si smallint;
		s string;
		a any?;
		
		i = si >>> 2; // 0
		i = i >>> si; // 0
		si = i >>> 2; // 0
		si = si >>> i; // 0
		si = getInt() >>> getSmallint(); //0
		a = s >>> 2; //1
		a = 2 >>> a; //1
		
		i >>>= 2; // 0
		i >>>= si; // 0
		i >>>= i; // 0
		i >>>= a; // 1
		i >>>= s; // 1
		i >>>= "2"; // 1
		i >>>= getString(); //1
		
		si >>>= 2; // 0
		si >>>= i; // 0
		si >>>= si; // 0
		si >>>= getInt(); // 0
		si >>>= a; // 1
		si >>>= s; // 1
		si >>>= "2"; // 1
		
		s >>>= 2; //1 
		a >>>= 2; //1
	end
	
	function funcIn(i int in) returns(int)
	end
	function funcInout(i int inout) returns(smallint)
	end
	function getString() returns(string)
	end
	function getInt() returns(int)
	end
	function getSmallInt() returns(smallint)
	end
end
