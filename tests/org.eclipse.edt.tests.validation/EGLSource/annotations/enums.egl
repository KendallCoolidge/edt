package annotations;

record annot1 type annotation
	{targets = [ ElementKind.EnumerationPart]} //0
	value string;
end


record annot2 type annotation 
	{targets = [ ElementKind.EnumerationEntry]} //0
	value string;
end

enumeration enum1 {@annot1{value = "test1"}}  //0
	A {@annot2{value = "test2"}}, //0
	B{@annot2{}} = 16, //0
	C = ~7, //0
	D = -6, //0
	E = +9 //0
end
