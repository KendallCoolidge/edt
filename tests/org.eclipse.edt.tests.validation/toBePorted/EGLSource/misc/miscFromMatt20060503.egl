//NO_ERROR

Program pgm
	function showit( a int[] )
		for ( i int to size( a ) )
			writeStdout( "[" + i + "] is " + a[ i ] );
		end
	end
	
	function showit( a int[][] )
		for ( i int to size( a ) )
			writeStdout( "[" + i + "] is..." );
			showit( a[ i ] );
		end
	end
	
	function main()
	end
end
