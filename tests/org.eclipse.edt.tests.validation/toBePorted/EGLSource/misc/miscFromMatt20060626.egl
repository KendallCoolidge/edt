Program pgm
	function main();
		moveit1a, moveit1b moveit1;
		moveit2a, moveit2b moveit2;

		move moveit1a to moveit1b;	//0
		move moveit2a to moveit2b;	//0

		move moveit2a to moveit1b;	//1
		moveit1b = moveit2a;		//1

		move moveit1a to moveit2b;	//1
		moveit2b = moveit1a;		//1
	end
end

Record moveit1
	field1 int;
end

Record moveit2
	field1 int;
end
