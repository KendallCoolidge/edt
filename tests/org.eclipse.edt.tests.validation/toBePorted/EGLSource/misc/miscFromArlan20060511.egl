//NO_ERROR

Program pgm
	function main()
		myForm myForm;
		f_1 any = myForm.f_1;
	
		openUI activeForm.f_1 {SQLColumnName = "xxxx"} bind f_1 end;
		
		clearFields ( activeForm.lastName );
	end
end

Record myForm type ConsoleForm {formSize=[100,100]}
	f_1 ConsoleField {fieldLen = 10, position=[1,1]};
end
