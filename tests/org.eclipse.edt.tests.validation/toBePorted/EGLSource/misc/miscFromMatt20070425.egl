Program pgm
	function main()
		myHex8 hex(8);
		myHex16 hex(16);
		
		MathLib.ceiling( myHex8 );	//0
		MathLib.ceiling( myHex16 );	//0
		
		myFloat float;
		mySmallFloat smallfloat;
		
		myHex8 = mySmallFloat;	//0
		mySmallFloat = myHex8;	//0

		myHex8 = myFloat;		//0
		myFloat = myHex8;		//0
		
		myHex16 = mySmallFloat;	//0
		mySmallFloat = myHex16;	//0

		myHex16 = myFloat;		//0
		myFloat = myHex16;		//0
	end
end