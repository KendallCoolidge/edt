handler myHandler

	//this test makes sure that you cannot pass "this" to an INOUT or OUT parm


	function f1()
		f2(
			this,   //1
			this,	//1
			this,	//0
			this	//1
			);
	end
	
	function f2(p1 myHandler, p2 myHandler inout, p3 myhandler in, p4 myhandler out)
	end
	
end
