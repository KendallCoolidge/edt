//NO_ERROR

record rec1 type BasicRecord
  10 id int;
  10 name CHAR(20);
end

Record rec2 type BasicRecord
  10 r1 rec1 {id = 1, name = "John"};
  10 r2 rec1[2] {this[1] {id = 2, name = "William"}, this[2] {id = 3, name = "Emily"}};
end
