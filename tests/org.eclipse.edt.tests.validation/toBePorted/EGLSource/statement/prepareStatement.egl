Record EducationPSB type PSBRecord 					 
	{defaultPSBName = "EDUCPSB"}					 
	iopcb IO_PCBRecord {@PCB{ pcbType=TP }};		 
 	altpcb ALT_PCBRecord {@PCB{ pcbType=TP }};		 
	studentPCB DB_PCBRecord { @PCB {					 
	  pcbType=DB,									 
	  pcbName="STUPCB",								 
	  hierarchy = [									 
	   @Relationship{ segmentRecord="Student" } 	 
	  ] }};

end

