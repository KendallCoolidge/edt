Program pgm
	var1 int[] {maxSize=10};
	function main()
		f1 char(10)[] {maxSize = 2147483647} ; //0
		f2 int[] {maxSize = 1} ; //0
		f3 int[] {maxSize = -1} ; //1 The value of maxSize must be a positive integer.
		f4 int[] {maxSize = 2147483648} ; //1 The integer literal 2147483648 is out of range.
	end
end
