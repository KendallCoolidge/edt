Handler hndlr1 type JSFHandler
	fixedRec fixedRec;
	
	definedVar int;
end

Handler hndlr2 type JSFHandler
	flexRec flexRec;
	
	definedVar int;
end

function func()
	undefinedVar = 10;		//2 hndlr1 - | hndlr2 -
	definedVar = 10;		//0
end

function func2()
	undefinedVar = 10;		//1 hndlr2 -
	definedVar = 10;		//0
end

function func3()
	undefinedVar = 10;		//1 hndlr2 -
	definedVar = 10;		//0
end

function func4()
	undefinedVar = 10;		//0
end

Record fixedRec
	10 topItem int {validatorFunction = func4};
		15 item1 int {validatorFunction = func};
end

Record fixedRec2
	10 topItem int {validatorFunction = func4};
		15 item1 int {validatorFunction = func3};
end

Record flexRec
	item1 int {validatorFunction = func};
	item2 flexRec2;
	item3 fixedRec2;
end

Record flexRec2
	item1 int {validatorFunction = func2};
end
