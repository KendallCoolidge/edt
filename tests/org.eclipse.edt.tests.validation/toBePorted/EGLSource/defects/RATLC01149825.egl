program redefProg type BasicProgram {}
	use redefLib ;
	aRec redefRecA ;
	bRec redefRecB { redefines = aRec } ;				//0
	bRec2 redefRecB { redefines = redefLib.aRec } ;		//1
	function main()
	end	
end

library redefLib  
	aRec redefRecA ;
	bRec redefRecB { redefines = aRec } ;
	function functionName()
	end	
end

record redefRecA
	10 aName char(10) ;
end

record redefRecB
	10 bName char(10) ;
end
