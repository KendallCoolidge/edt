Program pgm
	function retAnyHex() returns (any)
		hex1 hex(2);
		hex1 = "E1";
		return (hex1);
	end
	
	function main()
		e hex(4);
		e = retAnyHex() as hex(4) + "AB";  // 0
		e = "AB" + retAnyHex();  // 0
	end
end
