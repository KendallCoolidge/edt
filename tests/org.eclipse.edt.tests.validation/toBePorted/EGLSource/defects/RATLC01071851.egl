Record ArrayFixedArrayRecord
	10 * [20] ;
		20 aChar char(10) ;
		20 aMoney money ;
		20 aNum num (16,2) ;
		20 aUnicode unicode(10) ;
end 

Program pgm

	aFix ArrayFixedArrayRecord { initialized = yes } ;
	
	function main()
		move aFix.aChar to aFix.aUnicode for all ;	//0
	end
end
	
