Service myService
	serData int;
	
	function serFunc()
		serData = 10;		//0
		this.serData = 10;	//0
		serFunc();			//0
	end
end

Program pgm
	function main()
		myService myService;
		
		myService.serData = 10;		//1 cannot be resolved
		myService.serFunc();		//0
	end
end
