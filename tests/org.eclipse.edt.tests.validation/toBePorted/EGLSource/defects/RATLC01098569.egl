Program pgm type textUIProgram
{
	inputForm = form1,		//0
	@DLI{psb="myPSB"}
	
}

	myPSB psbRec;

	use fg1;				//0
	
	dliSegRec dliSegRec;
	
	function main()
	end
end

function valFunc()
	get dliSegRec;
	if(dliSegRec is ioerror) end
end

Record dliSegRec type DLISegment
end

Record psbRec
end

Formgroup fg1
	form form1 type textForm {formSize=[10,20]}
		field1 int {validatorFunction = "valFunc"};
	end
end