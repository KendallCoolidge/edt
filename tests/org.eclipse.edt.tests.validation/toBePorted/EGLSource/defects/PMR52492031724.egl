record rec1 type sqlrecord {keyitems=[item1, item2]}
	10 item1 int;
	10 item2 int;
	10 item3 int;
end

program pgm1 {allowUnqualifiedItemReferences = yes}

	rec1 rec1;
	
	function main()  
		open resultSetIdentifier          //1 Unmatched parentheses in SQL statement
			into item1, item2, item3
			for rec1 with
			#sql{    
				select
					item1, item2, item3
				from rec1 t1        
				where 
						(fred = :itgem1
					and	wilma = :item3
					
				order by
					item1, item2 asc
			};
					
	end
	
end