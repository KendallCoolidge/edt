Program pgm
	function main()
		dict dictionary[2];
		if ( dict[1].containsKey("name") )						//0
		end
		
		if ( (dict[1] as dictionary).containsKey("name") )		//0
		end
		
		if ( dict[1].containsKey(["name"]) )					//1
		end
		
		if ( (dict[1] as dictionary).containsKey(["name"]) )	//1
		end
	end
end
