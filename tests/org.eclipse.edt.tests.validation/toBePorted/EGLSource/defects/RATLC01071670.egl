//NO_ERROR

Program pgm() {I4GLItemsNullable=yes}
	function ct_parse()
	    n       int;
		n = null;
		
		ct1_destin String;
		
	    for( n from 1 to getCmdLineArgCount()) 
	        if ( getCmdLineArg(n) == "destin")	        
	            ct1_destin = getCmdLineArg(n+1); // 0
	        end 
	    end 
	    if ( ct1_destin == null)  
	       ct1_destin = "report.rpt";
	    end 
	    return;
	end
	
	function main()
	end
end 