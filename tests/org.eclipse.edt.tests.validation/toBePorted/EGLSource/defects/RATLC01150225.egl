handler TestPage type JSFHandler
	{onConstructionFunction = onConstruction,
 	 view = "TestPage.jsp"} 

	// Function Declarations
	function onConstruction()
	J2EELib.clearEGLSessionAttrs();//0
	J2EELib.clearApplicationAttr("sss");//0
	J2EELib.clearRequestAttr("sss");//0
	J2EELib.clearSessionAttr("ssss");//0
	J2EELib.getQueryParameter("wwww");//0
	result any;
	J2EELib.getRequestAttr("sss",result );//0
	J2EELib.getRequestAttr("aaaa");//0
	J2EELib.getSessionAttr("www",result);//0
	J2EELib.getSessionAttr("eeee");//0
	J2EELib.getApplicationAttr("wwww");//0
	J2EELib.setApplicationAttr("eeee",result);//0
	J2EELib.setRequestAttr("qqqq",result);//0
	J2EELib.setSessionAttr("rrrr","wwww");//0
	end
	


		
end

