handler hand1
	f1 int{
	@myann{
			view = wilma   //0
		},
	@myann{
			view = betty   //0
		},
	@myann{
			view = fred   //1
		}
		
		
	};
	
	fred fred;
	wilma wilma;
	betty betty;
end


externalType fred type javascriptobject
end

ExternalType wilma Extends betty type JavaScriptObject
end

ExternalType betty type JavaScriptObject
end

record myAnn type annotation
{
	targets = [
		ElementKind.FieldMbr
	]
}

	view betty;
end


