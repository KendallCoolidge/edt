Record SingleSegmentMessage type serialRecord {
	filename = "eglssmsq",
	lengthItem = msgLen
	}
	10 msgLen MsgRecLen ;
	10 id char(4) ;
	10 segCount char(4) ;
	10 text char(4) ;
end

DataItem MsgRecLen int end
