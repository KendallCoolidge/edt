Library lib	
	function func()
	    diff01 interval("yyyyMM");
	    t1, t2 timestamp("yyyyMMdd");	    
	    diff01 = t2 - t1;				//1
	    
	    t3, t4 timestamp("yyyyMM");
	    diff01 = t4 - t3;				//0
	end	
end
