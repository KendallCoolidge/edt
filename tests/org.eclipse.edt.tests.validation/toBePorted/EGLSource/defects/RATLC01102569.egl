Record FdtDateTimesInRecord 
	10 aDateAsChar char(8) ;
		20 aDate date ;
	10 aTimeAsChar char(6) ;
		20 aTime time ;
	10 aTimeStampAsChar char(14) ;
		20 aTimeStamp timeStamp ;
	10 aLongTimeStampAsChar char(20) ;
		20 aLongTimeStamp timeStamp ("yyyymmddhhmmssffffff") ;
	10 anIntervalAsChar char(7) ;
		20 anInterval interval ;
	10 aSecondSpanAsChar char(22) ;								//0
		20 aSecondSpan interval ("dddddddddhhmmssffffff") ;
end
