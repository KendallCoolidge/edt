Program pgm

	function main()
		blanks fixedRecord1;
		transfer to transactionTest passing blanks ;	//0
	end
end

Record fixedRecord1
	10 item1 int;
end

Record fixedRecord2
	10 item1 int;
end

Program transactionTest
{inputRecord=rec}
	rec fixedRecord2;
end
