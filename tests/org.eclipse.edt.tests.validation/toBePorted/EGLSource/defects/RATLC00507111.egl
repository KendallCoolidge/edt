Program exitOpenUI {I4GLItemsNullable=YES, LocalSQLScope=YES}

FUNCTION main()
   try 
   	OpenUI new Menu{ labelText= "DELETE", menuItems=[] }
   		onEvent (after_delete)
		   	exit openUI;		//0
			continue openUI;	//1
   	end
   onException (jex egl.core.AnyException)
		exit openUI;		//1
		continue openUI;	//2
   end
END 
END
