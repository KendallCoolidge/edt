Function BAL-INITMAP()
  returns(char(3))
  // Function Declarations
  VAGen_INDEX char(3) ;

  /* Reserved word used in an assignment statement*/
  VAGen_INDEX = "ABC";

  /* Reserved word used in EZERTN*/
  return( INDEX );
  return(VAGen_INDEX);
  return( INDEX);
  return(INDEX );
  return( INDEX );
  return(VAGen_INDEX);
  return( INDEX);
  return(INDEX );

  /* Starts with special characters used in EZERTN*/
  return( @INDEX );			//1
  return(VAGen_@INDEX);
  return( @INDEX);			//1
  return(VAGen_@INDEX );
//  return( #INDEX );		// GETS SYNTAX ERROR
  return(VAGen_#INDEX);
//  return( #INDEX);		// GETS SYNTAX ERROR
  return(VAGen_#INDEX );
end
