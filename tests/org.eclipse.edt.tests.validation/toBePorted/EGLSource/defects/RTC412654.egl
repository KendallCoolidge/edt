program pgm5 type uiprogram //0

	use lib1;
	
	flex1 flex1;
	struct1 struct1;
	sqlrec1 sqlrec1;
	
	
	function main()
		converse flex1;   //0
		converse struct1;  //1
		converse sqlrec1;  //0
		
		converse lib1.libF1; //0
		converse libF1;  //0
		converse lib1.libF2; //2
		converse libF2;  //2
		converse lib1.libF3; //0
		converse libF3;  //0

		show flex1;   //0
		show struct1;  //1
		show sqlrec1;  //0

		show lib1.libF1; //0
		show libF1;  //0
		show lib1.libF2; //2
		show libF2;  //2
		show lib1.libF3; //0
		show libF3;  //0

		
		show flex1 returning to "fred" passing flex1;   //1
		

	end

end

program pgm6 type uiprogram //0
	flex1 flex1;
	
	function main()
		show flex1 returning to pgm5;   //0
		show flex1 returning to pgm7;   //1
		
		transfer to program pgm5;      //0
		transfer to transaction pgm5;  //2
	end
end

