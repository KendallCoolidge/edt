formgroup libxfg
	form form1 type textform {formsize = [24,80]}
		a int;
	end
end

record libxUIRec type vguirecord
	10 a int;
end

record libxrec1
	a int;
end

record libxrec2 type sqlrecord
	a int;
end

library libx
	use libxfg;
	uirec libxuirec;
	rec1 libxrec1;
	rec2 libxrec2;
	
	function f1()
	
		converse form1; //1
		converse uirec; //1
		converse rec1; //0
		converse rec2; //0
		
		show form1; //1
		show uirec; //1
		show rec1; //0
		show rec2; //0
	
	end

end
	
