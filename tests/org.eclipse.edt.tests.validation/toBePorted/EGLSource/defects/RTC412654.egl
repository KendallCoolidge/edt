program pgm8 type uiprogram {includeReferencedFunctions = yes}

	function main()
		transfer to pgm9; //1
		transfer to program pgm9; //1

		transfer to pgm10; //1
		transfer to program pgm10; //1
		
		transfer to transaction pgm11; //1

		pgm8F1();
	end
	
end

function pgm8F1() 
		transfer to pgm9; //1
		transfer to program pgm9; //1

		transfer to pgm10; //1
		transfer to program pgm10; //1
		
		transfer to transaction pgm11; //1

end

