record cf type ConsoleForm
	name ConsoleField;
	anotherField ConsoleField;
end

Program pgm
	function main()
		myCF cf {
			//set the value of property 'name'
			name = "xyz",					//0
			
			//set the value of consoleField 'name'
			this.name = myCF.anotherField	//0
		};
	end
end