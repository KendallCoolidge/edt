program TestNullPtrException
	results int;

	function main ()
		testInConditionals();
	end
	
	function testInConditionals()
		if ( retString() matches "EGL*"  ) //1 cannot be resolved
			results = 1;
		else
			results = 0;
		end
		
		if ( func() matches "EGL*"  ) //0
			results = 1;
		else
			results = 0;
		end
	end
	
	function func() returns( string ) end
	
end