Program pgm
	b blob;
	function main()
	ts timestamp;
	iv interval("yyyymm");
	
		b *= b;   //2
		b /= b;   //2
		b -= b;   //2
		b += b;   //2
		b %= b;   //2
		b **= b;  //2
		
		ts += iv;  //0
		
		ts += 5;  //1
		
		ts = ts + 3; //1
		
		
	end
end
