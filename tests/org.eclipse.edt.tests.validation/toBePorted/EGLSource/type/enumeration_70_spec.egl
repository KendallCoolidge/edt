	MyNO = "NO"		//0
End

Program MyTextProgram
	myColor MyColorKind; 										//0
	use MyColorKind;											//0
	Function changeColor(f char(20) field, color MyColorKind)	//0
	  	case (color)											//0
			when (MyBLUE) set f blue;							//0
			when (MyRED) set f red;								//0
		end
	end
	
	Function main()
		if(myColor == MyGreen)	//0
			myColor = MyRed;	//0
		end
	end
end


