Program pgm
	function main()
		x any;
		i int;
		
		x = 1;				//0

		//Valid, b/c preprocessor adds an 'as' conversion
		i = x;				//0

		i = x as int; 		//0
		i = x + 1; 			//1
		i = 1 + x;			//1
		i = (x as int) + 1;	//0
	end
end