//NO_ERROR

Program pgm
	function main()
		sp speak = bark;
		func(sp);
		
		func(bark);
		func(meow);
		
		func(bark as speak);
		func(meow as speak);
	end
	
	
	function func(p1 speak)
		p1();
	end
	
	function bark()
		writeStdOut("bark!");
	end
	
	function meow()
		writeStdOut("meow...");
	end
end

Delegate speak() end
