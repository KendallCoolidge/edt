<?xml version="1.0" encoding="UTF-8"?>
<egl:deployment xmlns:egl="http://www.eclipse.org/xmlns/edt/deployment/1.0">
  <bindings>
    <binding name="DerbyConnection" type="edt.binding.sql" uri="workspace://DerbyConnection" useURI="true">
      <parameters>
        <parameter name="deployAsJndi" value="true"/>
        <parameter name="jndiName" value="jdbc/DerbyConnection"/>
        <parameter name="jndiApplicationAuth" value="false"/>
      </parameters>
    </binding>
  </bindings>
</egl:deployment>