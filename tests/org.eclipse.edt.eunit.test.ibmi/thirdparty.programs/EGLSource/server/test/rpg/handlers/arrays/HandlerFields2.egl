	function rec_recATest(){@Test {}}
		for(idx1 int from 1 to 2)
            LogResult.assertBigIntEqual("Fail rec[" + idx1 + "]recA.getSize()", 4, rec[idx1].recA.getSize());
			for(idx2 int from 1 to 2)
				recTest(rec[idx1].recA[idx2], "rec[" + idx1 + "].recA[" + idx2 + "]");
			end
		end
