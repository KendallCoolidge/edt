package egl.lang;

Record FieldDeclaration type Annotation {
	targets=[ElementKind.AnnotationValue]
}
	FieldName String;
	FieldType TypeRef;
	IsPrivate boolean;
end

Record FunctionDeclaration type Annotation {
	targets=[ElementKind.AnnotationValue]
}
	FunctionName String;
	IsStatic boolean;
	fieldType TypeRef;
	ParameterTypes TypeRef[];
	ReturnType TypeRef;
	isPrivate boolean;
	modifiers string[]; // making this a String[] for now, when enumeration entry references are fixed, this needs to be ParameterModifierKind[]
end
