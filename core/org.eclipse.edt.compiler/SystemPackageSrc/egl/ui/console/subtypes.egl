package egl.ui.console;

import egl.ui.*;
import egl.lang.reflect.NoReflect;


Record ConsoleForm type Annotation
{
	targets = [ElementKind.recordPart],
	@Stereotype {
		referenceType = yes,
		memberAnnotations = [
			binding, color, dataType, fieldLen, orientation, position, segments, validValues
		]
	},
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.ConsoleFormAnnotationTypeBinding",
	eglIsSystemPart = yes
}
	delimiters string;
	name string {noReflect = yes};
	formSize int[2];
	showBrackets boolean = yes;
end
