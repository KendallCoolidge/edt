package egl.javascript;

Record JavaScriptProperty type Annotation
{
	targets = [
		ElementKind.FieldMbr
	],
	eglIsSystemAnnotation = yes
} 
	getMethod String;
	setMethod String;
end

Record JavaScriptObject type Annotation
	{ 
		targets = [	ElementKind.ExternalTypePart ],
	 
		@stereotype
			{
		  		// State annotations that may be placed on
	 	 		// members of an ExternalType definition
	 	 		memberAnnotations = [ JavaScriptProperty ]
			},
		eglIsSystemAnnotation = yes,
		validationProxy = "org.eclipse.edt.compiler.binding.annotationType.JavaScriptObjectAnnotationTypeBinding"
	}

	relativePath String;
	javaScriptName String;
	includeFile String;
end
