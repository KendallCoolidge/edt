package egl.io.dli;

Record DLISegment type Annotation
{
	targets = [ElementKind.structuredRecordPart],
	@Stereotype {
		memberAnnotations = [dliFieldName]
	},
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.DLISegmentAnnotationTypeBinding",
	eglIsSystemAnnotation = yes
}
	hostVarQualifier string;
	keyItem egl.core.reflect.internalRef;
	lengthItem egl.core.reflect.internalRef;
	segmentName string;
end

Record PSBRecord type Annotation
{
	targets = [ElementKind.recordPart],
	@Stereotype {
		memberAnnotations = [pcb]
	},
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.PSBRecordAnnotationTypeBinding",
	eglIsSystemAnnotation = yes
}
	defaultPSBName string;
end
