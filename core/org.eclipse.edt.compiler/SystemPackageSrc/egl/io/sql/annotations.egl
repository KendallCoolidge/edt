package egl.io.sql;

Record AsBytes type Annotation
{
	targets = [
		ElementKind.fieldMbr,
		ElementKind.structuredFieldMbr,
		ElementKind.dataItemPart
	],
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.AsBytesAnnotationTypeBinding",
	eglIsSystemAnnotation = yes
}
	value boolean;
end

