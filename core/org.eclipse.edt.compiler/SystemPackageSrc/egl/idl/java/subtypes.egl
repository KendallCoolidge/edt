package egl.idl.java;

import egl.lang.reflect.NoReflect;

Record JavaObject type Annotation { 
	targets = [ ElementKind.ExternalTypePart ],
	
	@Stereotype {
		memberAnnotations = [
			EventListener, JavaName, JavaProperty, ThrowsExceptions
		]
	},
	eglIsSystemAnnotation = yes,
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.JavaObjectAnnotationTypeBinding",
	eglImplicitExtendedType = Object
}
	packageName String {NoReflect = yes};   //mark this as noReflect, so that setting this field does not change the EClass that specifies this stereotype
	javaName String;
end
