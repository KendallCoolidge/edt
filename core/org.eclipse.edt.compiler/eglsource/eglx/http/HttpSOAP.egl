package eglx.http;

import eglx.jws.SOAPEnvelope;
import eglx._service.Encoding;

record HttpSOAP
	request HttpRequest{method=HttpMethod.POST, encoding=Encoding.XML, charset = "UTF-8", contentType = "application/soap"};
	response HttpResponse{encoding=Encoding.USE_CONTENTTYPE};
	soapMessage SOAPEnvelope;
end