package eglx.rest;

Record PostRest type Annotation
{ 
	targets = [ ElementKind.functionMbr], 
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.PostRestAnnotationTypeBinding",
	eglIsSystemAnnotation = yes
}
	uriTemplate string?;
	requestFormat eglx._service.Encoding?;
	requestCharset string = "UTF-8";
	requestContentType string?;//default based on encoding
	responseFormat eglx._service.Encoding?;
	responseCharset string?;
	responseContentType string?;//default based on encoding
end
