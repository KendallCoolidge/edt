package eglx.rest;

Record PutRest type Annotation
{ 
	targets = [ ElementKind.functionMbr], 
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.PutRestAnnotationTypeBinding"
}
	uriTemplate string?;
	requestFormat eglx.services.Encoding?;
	requestCharset string = "UTF-8";
	requestContentType string?;//default based on encoding
	responseFormat eglx.services.Encoding?;
	responseCharset string?;
	responseContentType string?;//default based on encoding
end
