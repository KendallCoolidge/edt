package eglx.lang;

import egl.lang.reflect.ClassType;

/**
 * AnyException is the supertype of all exceptions.
 */
externalType AnyException extends EAny type NativeType
	/**
	 * Some text explaining the reason for this exception.  Messages may be
	 * presented in the user's language and locale.
	 */
	message String;

	/**
	 * A unique identifier for the condition which caused this exception.  It 
	 * should not be translated or localized.
	 */
	messageID String;
end
