package egl.ui.webTransaction;

import egl.core.*;
import egl.ui.*;

Record VGUIRecord type Annotation
{
	targets = [ElementKind.structuredRecordPart],
	@Stereotype {
		memberAnnotations = [
			displayName, programLinkData, numElementsItem, uiType, selectedIndexItem, displayName, value,
			help, alias, inputRequired, inputRequiredMsgKey, minimumInput, minimumInputMsgKey, needsSOSI,
			typeChkMsgKey, validValues, validValuesMsgKey, runValidatorFromProgram, validatorFunction,
			validatorFunctionMsgKey, validatorDataTable, validatorDataTableMsgKey, validationOrder,
			dateFormat, fillCharacter, isBoolean, timeFormat, uppercase
		]
	},
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.VGUIRecordAnnotationTypeBinding",
	eglIsSystemAnnotation = yes
}
	alias string;
	commandValueItem internalRef;
	help string;
	runValidatorFromProgram boolean;
	title string;
	validatorFunction internalRef;
end

Record VGWebTransaction type Annotation
{
	targets = [ElementKind.programPart],
	@Stereotype,
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.VGWebTransactionAnnotationTypeBinding",
	eglIsSystemAnnotation = yes
}
	inputRecord InternalRef;
	msgTablePrefix string;
	inputUIRecord internalRef;
	enableJ2EE boolean;
end
