Record IO_PCBRecord
	10 terminalName char(8);
	10 * char(2);
	10 statusCode char(2);
	10 * char(8);
	12 localDate decimal(7);
	12 localTime decimal(7);
	10 inputMsgSeqNum int;
	10 msgOutDescName char(8);
	10 userid char(8);
	10 groupName char(8);
	10 * char(12);
	12 currentDate decimal(7);
	12 currentTime decimal(11);
