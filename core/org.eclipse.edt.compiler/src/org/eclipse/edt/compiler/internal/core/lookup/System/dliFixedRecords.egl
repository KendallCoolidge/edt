	10 keyAreaLen int;
	10 numSensitiveSegs int;
	10 keyArea char(3825);
end

Record GSAM_PCBRecord
	10 dbName char(8);
	10 * num(2);
	10 statusCode char(2);
	10 procOptions char(4);
	10 * char(20);
	10 recordSearchArg bigint;
	10 undefinedRecordLen int;
end
