package eglx.jws;

import eglx.xml._bind.annotation.*;
/**
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:tns1="http://services" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <soapenv:Header xmlns:tns10="http://www.w3.org/2005/08/addressing">
    <tns10:Action soapenv:actor="http://www.w3.org/1999/xlink">http://www.w3.org/1999/xlink</tns10:Action>
    <tns10:From>
      <tns10:Address>http://www.w3.org/1999/xlink</tns10:Address>
    </tns10:From>
    <tns10:To>http://www.w3.org/1999/xlink</tns10:To>
  </soapenv:Header>
  <soapenv:Body>
    <tns1:sayHelloTo>
      <p1>Joe</p1>
    </tns1:sayHelloTo>
  </soapenv:Body>
</soapenv:Envelope>
**/
record SOAPEnvelope{@XMLRootElement{name="Envelope", namespace="http://schemas.xmlsoap.org/soap/envelope/"}}
	soapHeader any{XMLElement{name="Header", namespace="http://schemas.xmlsoap.org/soap/envelope/"}} = null;
	body any{XMLElement{name="Body", namespace="http://schemas.xmlsoap.org/soap/envelope/"}} = null;
end