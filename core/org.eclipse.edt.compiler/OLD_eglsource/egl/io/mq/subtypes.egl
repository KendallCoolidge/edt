package egl.io.mq;

Record MQRecord type Annotation
{
	targets = [ElementKind.structuredRecordPart],
	@Stereotype,
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.MQRecordAnnotationTypeBinding",
	eglIsSystemAnnotation = yes
}
	getOptionsRecord string;
	includeMsgInTransaction boolean = yes;
	lengthItem internalRef;
	msgDescriptorRecord string;
	numElementsItem internalRef;
	openOptionsRecord string;
	openQueueExclusive boolean;
	putOptionsRecord string;
	queueDescriptorRecord string;
	queueName string;
end
