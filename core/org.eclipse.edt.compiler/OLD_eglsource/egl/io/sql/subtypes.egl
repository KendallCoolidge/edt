package egl.io.sql;

Record SQLRecord type Annotation
{
	targets = [ElementKind.recordPart, ElementKind.structuredRecordPart],
	@Stereotype {
		memberAnnotations = [
			column, isSqlNullable, isReadOnly, maxLen, persistent, sqlDataCode,
			sqlVariableLen
		]
	},
	validationProxy = "org.eclipse.edt.compiler.binding.annotationType.SQLRecordAnnotationTypeBinding",
	eglIsSystemAnnotation = yes
}
	defaultSelectCondition SQLString;
	fieldsMatchColumns boolean = false;
	keyItems internalRef[];
	tableNames string[][];
	tableNameVariables string[][];	
end
