package egl.ui.jsf;

import egl.ui.*;

Record JSFHandler type Annotation
{
	targets = [ElementKind.HandlerPart],
	@Stereotype {
		memberAnnotations = [
			action, bypassValidation, displayName, displayUse, help, fill,
			inputRequired, inputRequiredMsgKey, isDecimalDigit, isHexDigit,
			minimumInput, minimumInputMsgKey, needsSOSI, newWindow,
			numElementsItem, onValueChangeFunction, selectFromListItem,
			selectType, typeahead, typeaheadFunction, typeChkMsgKey,
			validatorDataTable, validatorDataTableMsgKey, validatorFunction,
			validatorFunctionMsgKey, validationOrder, validValues,
			validValuesMsgKey, value
		],
		stereotypeContexts = []
	},
	validationProxy="org.eclipse.edt.compiler.binding.annotationType.JSFHandlerAnnotationTypeBinding",
	eglIsSystemAnnotation = yes
}
	bidiConversionTable string;
	cancelOnPageTransition boolean = no;
	eventValueItem string;
	labelAndHelpResource string;
	msgResource string;
	onConstructionFunction internalRef;
	onPostRenderFunction functionRef;
	onPreRenderFunction functionRef;
	scope ScopeKind = ScopeKind.session;
	title string;
	validationBypassFunctions internalRef[];
	validatorFunction internalRef;
	view string;
	viewRootVar internalRef;
end
