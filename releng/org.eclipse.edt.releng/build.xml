<!-- A build file for use with Hudson. Note that forceContextQualifier 
	 is being set, which overrides tag/branch values found in *.map files 
-->
<project default="run">
	<target name="run">

<!-- Uncomment these lines when we're ready for regular builds.
		<tstamp>
			<format property="buildTimestamp" pattern="yyyyMMddHHmm" />
		</tstamp>
		<property name="forceContextQualifier" value="v${buildTimestamp}" />
-->
		
		<property name="buildDir" value="${writableBuildRoot}/athena" /> 		
		<property name="build.properties" value="build.properties" />
		<property file="${build.properties}" />
		<ant antfile="${relengCommonBuilderDir}/build.xml" />
	</target>
</project>
