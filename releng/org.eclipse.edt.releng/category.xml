<?xml version="1.0" encoding="UTF-8"?>
<site pack200="true">
        <!-- ${category.name} -->
        <category-def label="${edtProjectName} ${version}" name="${category.name}">
                <description>
${edtProjectName} ${version}: contains all features in this build.
                </description>
        </category-def>
 
        <feature id="org.eclipse.edt.feature" version="${org.eclipse.edt.feature.version}" url="features/org.eclipse.edt.feature_${org.eclipse.edt.feature.version}.jar" patch="false">
                <category name="${category.name}"/>
        </feature>
 
        <feature id="org.eclipse.edt.sdk.feature" version="${org.eclipse.edt.sdk.feature.version}" url="features/org.eclipse.edt.sdk.feature_${org.eclipse.edt.sdk.feature.version}.jar" patch="false">
                <category name="${category.name}"/>
        </feature>
</site>